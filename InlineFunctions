#include <iostream>
 
using namespace std;

inline int Max(int x, int y)
{
   return (x > y)? x : y;
}

// Main function for the program
int main( )
{
    int x,y;
   cout << "Enter x: ";
   cin >> x;
   cout << "Enter y: ";
   cin >> y; 
   cout << "Max (" << x<<"," << y<< "): " << Max(x,y) << endl;
   //cout << "Max (0,200): " << Max(0,200) << endl;
   //cout << "Max (100,1010): " << Max(100,1010) << endl;
   return 0;
}
