#include <iostream>

using namespace std;

int main()
{
    for(int i = 0; i < 50; i++)
    {
        cout << "*";
    }
    cout << endl;
    
    cout << "        C C C          S S S S         !!\n";
    cout << "      C       C       S        S       !!\n";
    cout << "     C               S                 !!\n";
    cout << "    C                 S                 !!\n";             
    cout << "   C                   S                !!\n";             
    cout << "   C                     S              !!\n";
    cout << "   C                       S             !!\n";
    cout << "    C                         S           !!\n";
    cout << "     C                          S          \n"; 
    cout << "      C       C       S        S         \n";
    cout << "        C C C           S S S S       00\n\n"; 
    
    for(int i = 0; i < 50; i++)
    {
        cout << "*";
    }
    cout << endl;
    
    cout << "     Computer Science is Cool Stuff!\n";
    
    return 0;
}
